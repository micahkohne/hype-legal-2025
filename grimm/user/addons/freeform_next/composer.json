{
  "name": "solspace/ee-freeform-next",
  "description": "The most reliable, intuitive and powerful form builder for ExpressionEngine.",
  "version": "3.3.0",
  "type": "library",
  "authors": [
    {
      "name": "Solspace",
      "homepage": "https://solspace.com/"
    }
  ],
  "require": {
    "php": "^8.2||^8.3||^8.4",
    "symfony/property-access": "^6.0",
    "symfony/finder": "^5.0",
    "symfony/filesystem": "^2.8",
    "hashids/hashids": "^4.1",
    "pixel418/markdownify": "^2.3.1",
    "egulias/email-validator": "^2.1",
    "statamic/stringy": "^3.1",
    "apache/log4php": "^2.3.0",
    "doctrine/lexer": "^1.0.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.1",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "phpcompatibility/php-compatibility": "^10.2",
    "squizlabs/php_codesniffer": "^3.7",
    "rector/rector": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Solspace\\Addons\\FreeformNext\\": ""
    },
    "classmap": [
      "mcp.freeform_next.php",
      "upd.freeform_next.php",
      "jump.freeform_next.php",
      "mod.freeform_next.php",
      "ext.freeform_next.php",
      "ft.freeform_next.php"
    ],
    "exclude-from-classmap": [
      "vendor/",
      "php7/"
    ]
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "platform": {
      "php": "8.2.0"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
