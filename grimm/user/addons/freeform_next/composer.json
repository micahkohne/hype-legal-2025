{
  "name": "solspace/ee-freeform-next",
  "description": "The most reliable, intuitive and powerful form builder for ExpressionEngine.",
  "version": "3.3.3",
  "type": "library",
  "require": {
    "php": "^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4",
    "symfony/property-access": "^5.4 || ^6.0",
    "symfony/finder": "^5.4 || ^6.0",
    "symfony/filesystem": "^5.4 || ^6.0",
    "hashids/hashids": "^4.1",
    "pixel418/markdownify": "^2.3.1",
    "egulias/email-validator": "^2.1",
    "statamic/stringy": "^3.1",
    "apache/log4php": "^2.3.0",
    "doctrine/lexer": "^1.0.1",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "rector/rector": "^0.17.12",
    "nikic/php-parser": "^4.15.5"
  },
  "autoload": {
    "psr-4": {
      "Solspace\\Addons\\FreeformNext\\": ""
    },
    "classmap": [
      "mcp.freeform_next.php",
      "upd.freeform_next.php",
      "jump.freeform_next.php",
      "mod.freeform_next.php",
      "ext.freeform_next.php",
      "ft.freeform_next.php"
    ],
    "exclude-from-classmap": [
      "vendor/"
    ]
  },
  "config": {
    "platform": {
      "php": "8.0.30"
    },
    "allow-plugins": {}
  },
  "scripts": {
    "rector": "rector process",
    "rector:dry": "rector process --dry-run",
    "rector:clear": "rector process --clear-cache --dry-run"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
